<html>
    <head></head>
    <body>
        <h1>CURSO DE NODE JS - PRACTICA</h1>
        <table class="table">
            <thead>
               <tr>
                  <th>Nombre Pokemon</th>
                   <th>URL</th>
               </tr>
            </thead>
            <tbody>
                <% pokemones.forEach((pokemon) => { %>
                    <tr>
                      <td><%=pokemon.name%></td>
                      <td><%=pokemon.url%></td>
                    </tr>
                  <% }) %>
            </tbody>
          </table>
    </body>
</html>